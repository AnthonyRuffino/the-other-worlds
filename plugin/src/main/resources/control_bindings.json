{
    "gameModeKeyBindings": {
      "global": {
        "F11": {
          "listenerAction": "toggle_fullscreen"
        },
        "ESCAPE": {
          "listenerAction": "toggle_menu",
          "checkedAction": null
        }
      },
      "play": {
        "W": {
          "listenerAction": "forward"
        },
        "A": {
          "listenerAction": "left"
        },
        "S": {
          "listenerAction": "back"
        },
        "D": {
          "listenerAction": "right"
        },
        "NUM_1": {
          "listenerAction": "action_bar_1_1"
        },
        "NUM_2": {
          "listenerAction": "action_bar_1_2"
        },
        "NUM_1+ALT_LEFT": {
          "listenerAction": "action_bar_2_1"
        },
        "NUM_2+ALT_LEFT": {
          "listenerAction": "action_bar_2_2"
        },
        "PAGE_UP": {
          "listenerAction": "shift_camera_up"
        },
        "PAGE_DOWN": {
          "listenerAction": "shift_camera_down"
        },
        "PAGE_UP+ALT_LEFT": {
          "listenerAction": "shift_camera_left"
        },
        "PAGE_DOWN+ALT_LEFT": {
          "listenerAction": "shift_camera_right"
        }
      }
    },
    "gameModeButtonBindings": {
      "global": {
        "MIDDLE": {
          "worldAction": "toggle_autorun"
        },
        "BACK": {
          "worldAction": "zoom_out"
        },
        "FORWARD": {
          "worldAction": "zoom_in"
        }
      },
      "menu": {
        "LEFT": {
          "hudAction": "select_hud_element"
        },"RIGHT": {
          "hudAction": "cancel_hud_element"
        },
        "MIDDLE": {
          "hudAction": "toggle_autorun"
        },
        "BACK": {
          "hudAction": "scroll_hud_option_down"
        },
        "FORWARD": {
          "hudAction": "scroll_hud_option_up"
        }
      },
      "play": {
        "LEFT": {
          "worldAction": "select_entity",
          "hudAction": "select_hud_element"
        },
        "RIGHT": {
          "worldAction": "auto_attack_entity",
          "hudAction": "cancel_hud_element"
        }
      }
    }
  }
}